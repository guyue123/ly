<?xml version="1.0" encoding="UTF-8"?>

<root
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://editor.huliqing.name'
    xsi:schemaLocation='http://editor.huliqing.name meta.xsd'>
    
    <data name="magicBase" extends="base">
        <field name="useTime" converter="FloatTextConverter" />
        <field name="hitCheckEl" converter="TextConverter" />
        <field name="effects" converter="TextConverter" />
        <field name="sounds" converter="TextConverter" />
    </data>

    <data name="magicState" extends="magicBase">
        <field name="states" converter="TextConverter" />
        <field name="actorAnims" converter="TextConverter" />
    </data>
    
    <data name="magicAttributeHit" extends="magicBase">
        <field name="attributes" converter="TextConverter" />
        <field name="interval" converter="FloatTextConverter" />
        <field name="distance" converter="FloatTextConverter" />
    </data>
    
</root>

<?xml version="1.0" encoding="UTF-8"?>

<root
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://editor.huliqing.name'
    xsi:schemaLocation='http://editor.huliqing.name meta.xsd'>
    
    <converter name="ModuleSwayGeometriesConverter" value="name.huliqing.editor.converter.field.ModuleSwayGeometriesConverter" />
    
    <data name="moduleBase" extends="base">
        <field name="moduleOrder" converter="IntegerTextConverter" />
        <field name="bindMessageEnabledAttribute" converter="TextConverter" />
    </data>

    <data name="moduleActor" extends="moduleBase">
        <field name="radius" converter="FloatTextConverter" />
        <field name="height" converter="FloatTextConverter" />
        <field name="bindDeadAttribute" converter="TextConverter" />
        <field name="bindTargetAttribute" converter="TextConverter" />
        <field name="bindMassAttribute" converter="TextConverter" />
        <field name="bindMovableAttribute" converter="TextConverter" />
        <field name="bindRotatableAttribute" converter="TextConverter" />
        <field name="bindRotatableAttribute" converter="TextConverter" />
        <field name="bindNameAttribute" converter="TextConverter" />
    </data>

    <data name="moduleAction" extends="moduleBase">
        <field name="bindDeadAttribute" converter="TextConverter" />
    </data>

    <data name="moduleChannel" extends="moduleBase">
        <field name="extAnimDir" converter="TextConverter" />
    </data>

    <data name="moduleColor" extends="moduleBase">
        <field name="bindColorAttribute" converter="TextConverter" />
    </data>

    <data name="moduleDrop" extends="moduleBase">
        <field name="bindDeadAttribute" converter="TextConverter" />
    </data>
    
    <data name="moduleItem" extends="moduleBase">
    </data>
    
    <data name="moduleLevel" extends="moduleBase">
        <field name="bindLevelAttribute" converter="TextConverter" />
        <field name="bindXpAttribute" converter="TextConverter" />
        <field name="bindXpNextAttribute" converter="TextConverter" />
        <field name="bindLevelUpEnabledAttribute" converter="TextConverter" />
        <field name="xpLevelEl" converter="TextConverter" />
        <field name="maxLevel" converter="IntegerTextConverter" />
        <field name="effect" converter="TextConverter" />
    </data>
    
    <data name="moduleLod" extends="moduleBase">
        <field name="distTolerance" converter="FloatTextConverter" />
        <field name="trisPerPixel" converter="FloatTextConverter" />
    </data>

    <data name="moduleLogic" extends="moduleBase">
        <field name="bindEnabledAttribute" converter="TextConverter" />
    </data>

    <data name="modulePhysics" extends="moduleBase">
        <field name="mass" converter="FloatTextConverter" />
        <field name="friction" converter="FloatTextConverter" />
        <field name="restitution" converter="FloatTextConverter" />
        <field name="kinematic" converter="CheckBoxConverter" />
        <field name="physicsShape" converter="DataComponentConverter">
            <feature name="componentType" value="physicsShape" />
        </field>
        <field name="angularDamping" converter="FloatTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="angularFactor" converter="FloatTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="angularSleepingThreshold" converter="FloatTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="angularVelocity" converter="Vector3fConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="applyPhysicsLocal" converter="CheckBoxConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="ccdMotionThreshold" converter="FloatTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="ccdSweptSphereRadius" converter="FloatTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="collideWithGroups" converter="IntegerTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="collisionGroup" converter="IntegerTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="kinematicSpatial" converter="CheckBoxConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="linearDamping" converter="FloatTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="linearSleepingThreshold" converter="FloatTextConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="linearFactor" converter="Vector3fConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="linearVelocity" converter="Vector3fConverter">
            <feature name="collapsed" value="true" />
        </field>
    </data>

    <data name="moduleResist" extends="moduleBase">
    </data>

    <data name="moduleSkill" extends="moduleBase">
        <field name="idleSkillTypes" converter="TextConverter" />
        <field name="waitSkillTypes" converter="TextConverter" />
        <field name="walkSkillTypes" converter="TextConverter" />
        <field name="runSkillTypes" converter="TextConverter" />
        <field name="hurtSkillTypes" converter="TextConverter" />
        <field name="deadSkillTypes" converter="TextConverter" />
        <field name="bindDeadAttribute" converter="TextConverter" />
        <field name="bindHurtAttributes" converter="TextConverter" />
    </data>

    <data name="moduleSkin" extends="moduleBase">
        <field name="bindWeaponSlotAttribute" converter="TextConverter" />
    </data>

    <data name="moduleState" extends="moduleBase">
    </data>

    <data name="moduleSway" extends="moduleBase">
        <field name="sway" converter="CheckBoxConverter" />
        <field name="swayFrequency" converter="FloatTextConverter" />
        <field name="swayVariation" converter="FloatTextConverter" />
        <field name="swayDistance" converter="FloatTextConverter" />
        <field name="swayWind" converter="Vector2fConverter">
            <feature name="collapsed" value="true" />
        </field>
        <field name="swayGeometries" converter="ModuleSwayGeometriesConverter" />
    </data>

    <data name="moduleTalent" extends="moduleBase">
        <field name="bindLevelAttribute" converter="TextConverter" />
        <field name="bindTalentPointsAttribute" converter="TextConverter" />
        <field name="talentPointsLevelEl" converter="TextConverter" />
    </data>

    <data name="moduleTask" extends="moduleBase">
    </data>

    
</root>

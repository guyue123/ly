<?xml version="1.0" encoding="UTF-8"?>

<root
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://editor.huliqing.name'
    xsi:schemaLocation='http://editor.huliqing.name meta.xsd'>
    
    <data name="attributeBase" extends="base">
        <field name="name" converter="TextConverter" />
    </data>
    
    <data name="attributeBoolean" extends="attributeBase">
        <field name="value" converter="CheckBoxConverter" />
    </data>
    
    <data name="attributeColor" extends="attributeBase">
        <field name="value" converter="ColorConverter" />
    </data>
    
    <data name="attributeFloat" extends="attributeBase">
        <field name="value" converter="FloatTextConverter" />
    </data>
    
    <data name="attributeGroup" extends="attributeBase">
        <field name="attributes" converter="ListDataConverter">
            <feature name="componentType" value="attribute" />
            <feature name="collapsed" value="false" />
        </field>
    </data>
    
    <data name="attributeInteger" extends="attributeBase">
        <field name="value" converter="IntegerTextConverter" />
    </data>
    
    <data name="attributeLevelFloat" extends="attributeBase">
        <field name="value" converter="FloatTextConverter" />
        <field name="level" converter="IntegerTextConverter" />
        <field name="levelEl" converter="TextConverter"/>
    </data>
    
    <data name="attributeLevelInteger" extends="attributeBase">
        <field name="value" converter="IntegerTextConverter" />
        <field name="level" converter="IntegerTextConverter" />
        <field name="levelEl" converter="TextConverter"/>
    </data>
    
    <data name="attributeLimitInteger" extends="attributeBase">
        <field name="value" converter="IntegerTextConverter" />
        <field name="minValue" converter="IntegerTextConverter" />
        <field name="maxValue" converter="IntegerTextConverter" />
        <field name="bindMinLimitAttribute" converter="TextConverter"/>
        <field name="bindMaxLimitAttribute" converter="TextConverter"/>
    </data>
    
    <data name="attributeLong" extends="attributeBase">
        <field name="value" converter="LongTextConverter" />
    </data>
    
    <data name="attributeRelateBoolean" extends="attributeBase">
        <field name="value" converter="CheckBoxConverter" />
        <field name="refVars" converter="TextConverter" />
        <field name="valueEl" converter="TextConverter" />
    </data>
    
    <data name="attributeString" extends="attributeBase">
        <field name="value" converter="TextConverter" />
    </data>
    
    <data name="attributeStringList" extends="attributeBase">
        <field name="value" converter="TextConverter" />
        <field name="duplication" converter="CheckBoxConverter" />
    </data>
    
    <data name="attributeVector2f" extends="attributeBase">
        <field name="value" converter="Vector2fConverter" />
    </data>
    
    <data name="attributeVector3f" extends="attributeBase">
        <field name="value" converter="Vector3fConverter" />
    </data>
    
    <data name="attributeVector4f" extends="attributeBase">
        <field name="value" converter="Vector4fConverter" />
    </data>

    
</root>

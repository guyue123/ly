<?xml version="1.0" encoding="UTF-8"?>

<root
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://luoying.huliqing.name'
    xsi:schemaLocation='http://luoying.huliqing.name meta/shape.xsd'>
    
    <!-- =================================================================== -->
    <!--内置的网格物体：Shape-->
    <!-- =================================================================== -->
    
    <!--单位立方体Box-->
    <shapeBox id="sysShapeBox" extents="0.5,0.5,0.5" />
    
    <!--单位圆-->
    <shapeSphere id="sysShapeSphere" radius="1" zSamples="8" radialSamples="8" />
    
    <!--自定义网格: 需要指定一个资源文件(j3o)模型-->
    <shapeCustom id="sysShapeCustom" file="" />
</root>



